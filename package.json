{
  "name": "vscode-dice",
	"displayName": "vscode-dice",
	"description": "you can play diceRoll in your vscode.",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.64.0"
	},
	"icon": "images/logo.png",
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/ucucuc4251/vscode-dice"
	},
	"bugs": {
		"url": "https://github.com/ucucuc4251/vscode-dice/issues"
	},
	"activationEvents": [
        "*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": {
			"title": "vscode-dice completion",
			"properties": {
				"diceconf.languages":{
					"type": "array",
					"description": "A list of languages in which the completion will be available",
					"default": [
						"plaintext",
						"javascript",
						"typescript",
						"javascriptreact",
						"typescriptreact"
					],
					"uniqueItems": true,
          			"items": {
            			"type": "string",
            			"enum": [
							"plaintext",
							"javascript",
							"typescript",
							"javascriptreact",
							"typescriptreact"
            			]
          			}
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.64.0",
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.9.1",
		"@typescript-eslint/parser": "^5.9.1",
		"eslint": "^8.6.0",
		"glob": "^7.2.0",
		"mocha": "^9.1.3",
		"typescript": "^4.5.4",
		"@vscode/test-electron": "^2.0.3"
	}
}
